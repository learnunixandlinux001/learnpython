{
"update_url": "https://clients2.google.com/service/update2/crx",

  "version": "0.2.3",
  "manifest_version": 3,
  "short_name": "onion-button",
  "name": "Onion Browser Button",
  "permissions": ["proxy", "storage", "notifications"],
  "host_permissions": ["https://check.torproject.org/*"],
  "homepage_url": "https://mybrowseraddon.com/tor-button.html",
  "description": "Easily browse the internet using TOR proxy with just one click!",
  "commands": {
    "_execute_action": {}
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
     "default_title": "Onion Browser Button",
     "default_popup": "data/popup/popup.html",
     "default_icon": {
       "16": "data/icons/16.png",
       "32": "data/icons/32.png",
       "48": "data/icons/48.png",
       "64": "data/icons/64.png"
     }
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  }
}
